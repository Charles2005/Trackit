<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trackit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrackIt">
        <activity
            android:name=".activity_destination"
            android:exported="false" />
        <activity
            android:name=".NewTask"
            android:exported="false" />
        <activity
            android:name=".TasksLayout"
            android:exported="false" />
        <activity
            android:name=".AlarmItem"
            android:exported="false" />
        <activity
            android:name=".NotesItem"
            android:exported="false" />
        <activity
            android:name=".Dashboard"
            android:exported="false" />
        <activity
            android:name=".AccountPage"
            android:exported="false" />
        <activity
            android:name=".AlarmsPage"
            android:exported="false" />
        <activity
            android:name=".CheckListPage"
            android:exported="false" />
        <activity
            android:name=".NotesPage"
            android:exported="false" />
        <activity
            android:name=".SignUpPage"
            android:exported="false" />
        <activity
            android:name=".LandingPage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".AlarmReceiver" />
    </application>

</manifest>